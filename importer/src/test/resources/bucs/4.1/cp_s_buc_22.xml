			<parameters> 
				<parameter key="isBulk">true</parameter> <!-- default is false --> 		
			</parameters> 
			<triggers> 
				<createActionTrigger onAction="DOC_RECEIVE" onResult="SUCCESS" actionType="DOC_CREATE" documentType="S033" /> 
			</triggers> 
		</document> 
 
		<document type="S031"> 
			<parameters> 
				<parameter key="isBulk">true</parameter> <!-- default is false --> 		
			</parameters> 
		</document> 
		
 
		<document type="S034"> 
			<parameters> 
