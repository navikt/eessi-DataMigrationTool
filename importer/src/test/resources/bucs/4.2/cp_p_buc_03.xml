				<createActionTrigger onAction="DOC_RECEIVE" onResult="SUCCESS" actionType="DOC_RECEIVE_REPLY" documentType="H121" /> 
			</triggers> 
		</document> 
 
		<document type="H121"> 
			<parameters> 
				<parameter key="hasMultipleVersions">true</parameter> 
